{
  "name": "gemini",
  "baseUrl": "https://generativelanguage.googleapis.com",
  "headers": {
    "Content-Type": "application/json",
    "User-Agent": "api-router/1.0",
    "Accept": "application/json"
  },
  "endpoints": {
    "/v1/chat/completions": {
      "upstreamPath": "/v1beta/openai/chat/completions",
      "headers": {
        "Accept": "application/json, text/event-stream"
      },
      "streamSupport": true
    },
    "/v1/completions": {
      "upstreamPath": "/v1beta/openai/completions"
    },
    "/v1/embeddings": {
      "upstreamPath": "/v1beta/openai/embeddings"
    }
  },
  "modelMapping": {
    "gpt-4o": "gemini-1.5-pro",
    "gpt-4": "gemini-1.5-pro",
    "gpt-4o-mini": "gemini-1.5-flash",
    "gpt-3.5-turbo": "gemini-pro"
  },
  "port": 8000
}
